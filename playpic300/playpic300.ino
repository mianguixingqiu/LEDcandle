void setup()
{
  Serial.begin(115200);
  pinMode(LED_BUILTIN, OUTPUT);     // Initialize the LED_BUILTIN pin as an output
  //module.setDisplayToString("LLLL");
  //delay(500);                        // wait
  module.clearDisplay();
  matrix.setIntensity(1);  
}

void loop()
{
  //matrix.setAll(false);
  Serial.println(1);
  for(int i=0;i<300;i++)
  {
    drawBitmap(picBitmap[i]);
    Serial.println(i);
    //Serial.println(2);
  }
}
void drawBitmap( const uint8_t *bitmap)
{
  matrix.fillScreen(LOW);
  for (int y = 0; y < MATRIX_NUMCOLUMNS; y++)
  {
    uint8_t byte = bitmap[y];
    for (int x = 0; x < MATRIX_NUMROWS; x++) 
    {
      int bitIndex = 7 - (x % 8);
      bool pixel = (byte >> bitIndex) & 0x01;
      matrix.drawPixel(x , y , pixel);
    }
  }
  matrix.write();
}